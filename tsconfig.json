{
  "compilerOptions": {  
    "allowImportingTsExtensions": true,  
    "target": "ESNext",                              
    "module": "ESNext",                             
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": false,
    "esModuleInterop": true,                         
    "forceConsistentCasingInFileNames": true,        
    "lib": [ "esnext" ],
    "strict": true,                                  
    "skipLibCheck": true                             
  }
}
